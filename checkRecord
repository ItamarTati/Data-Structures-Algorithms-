var checkRecord = function(s) {
    let count = 0
    for(let index = 0; index < s.length; index++){
        if(s[index] === 'A'){
            count++
        }
        if(s[index] === 'L' && s[index + 1] === 'L' && s[index + 2] === 'L'){
            return false
        }
    }
    return count < 2
};

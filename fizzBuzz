var fizzBuzz = function(n) {
    let answer = []
    for (let number = 1; number <= n; number++){
        if(number % 5 === 0 && number % 3 === 0){
            answer.push('FizzBuzz')
        }
        else if (number % 5 === 0){
            answer.push('Buzz')
        }
        else if(number % 3 ===0){
            answer.push('Fizz')
        }
        else{
            answer.push(number.toString())
        }

    }
    return answer
};
